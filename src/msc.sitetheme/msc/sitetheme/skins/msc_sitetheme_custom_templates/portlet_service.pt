<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      i18n:domain="plone">
<body>
<div metal:define-macro="portlet">

<dl class="portlet" id="portlet-images"
    tal:define="portal_state context/@@plone_portal_state;
                portal portal_state/portal">

    <dd class="portletItem">
		<img tal:attributes="src portal/++resource++msc.sitetheme.images/quality-1.png;
                       		 alt string:Products we use" />
		<img tal:attributes="src portal/++resource++msc.sitetheme.images/quality-2.png;
		 					alt string:Products we use" />
		<img tal:attributes="src portal/++resource++msc.sitetheme.images/quality-3.png;
		 					 alt string:Products we use" />
		<img tal:attributes="src portal/++resource++msc.sitetheme.images/quality-4.png;
		               		 alt string:Products we use" />
    </dd>

</dl>

</div>
</body>
</html>